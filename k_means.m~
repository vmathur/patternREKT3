function [ output_args ] = k_means(k, x)
%UNTITLED5 Summary of this function goes here
%   Detailed explanation goes here
    x(:,4) = 0;
    
    n=length(x);
    r=randperm(n);
    r=r(1:k);
    prototypes = [];
    for k = 1:length(r)
        prototypes = [prototypes; x(r(k),:)];
    end
    prototypes = prototypes(1:10,1:2);
   
    for i= 1:n
        z = x(i,1:2);
        class = med(prototypes,z);
        x(i,4)= class;
    end
    
    prototypes2 = []
    for i= 1:n
        rows = x(x(:,4)==2,:);
        a_mean = mean(
        class = med(prototypes,z);
        x(i,4)= class;
    end
        x(x(:,4)==2,:)
    
end